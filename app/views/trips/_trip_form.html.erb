<%= form_for @trips do |f| %>
  <fieldset>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= date_select(:trip, :start) %>
      <input type="text" data-provide='datepicker' >
    </div>
    <div>
      <%= date_select(:trip, :end) %>
    </div>
  </fieldset>

  <%= submit_tag("Create Trip") %>
<% end %>
