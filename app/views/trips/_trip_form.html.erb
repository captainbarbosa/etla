<%= form_for @trips do |f| %>
  <fieldset>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= date_select(:trip, :start, order: [:day, :month, :year]) %>
    </div>
    <div>
      <%= date_select(:trip, :end, order: [:day, :month, :year]) %>
    </div>
  </fieldset>

  <%= submit_tag("Create Trip") %>
<% end %>
